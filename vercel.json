{
  "version": 2,
  "builds": [
    {
      "src": "src/main.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["prisma/**", "prisma.config.ts", "public/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    }
  ]
}
